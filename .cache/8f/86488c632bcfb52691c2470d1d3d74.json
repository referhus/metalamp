n\r\n.arrival-departure:nth-last-child(-n+3){\r\n  flex-basis: 100%;\r\n}\r\n\r\n.input-ghost {\r\n  border: 1px solid #1F204140;\r\n  padding: 10px;\r\n  box-sizing: border-box;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  cursor: pointer;\r\n}\r\n\r\n.dropdown input::placeholder {\r\n    @include body;\r\n    text-transform: uppercase;\r\n    padding: 8px 10px;\r\n}\r\n\r\n.ghost input::placeholder {\r\n  text-transform: none;\r\n}\r\n\r\n.arrow-down {\r\n  color: #1F204180;\r\n}\r\n\r\n.btn {\r\n  @include h3;\r\n  display: flex;\r\n  flex-basis: 100%;\r\n  padding: 15px;\r\n  box-sizing: border-box;\r\n  background: linear-gradient(180deg, #BC9CFF 0%, #8BA4F9 100%);\r\n  border-radius: 22px;\r\n  text-align: center;\r\n  justify-content: space-between;\r\n  color: #fff;\r\n  text-decoration: none;\r\n  margin: 10px 0px 10px;\r\n  text-align: center;\r\n\r\n p {\r\n  display: flex;\r\nmargin: auto;\r\n}\r\n}\r\n\r\n.material-icons {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  font-size: 24px;\r\n}\r\n\r\n.number {\r\n  max-width: 300px;\r\n\r\n  span {\r\n    font-size: 12px;\r\n    color: #BC9CFF;\r\n;\r\n    text-transform: uppercase;\r\n  }\r\n}\r\n\r\n.title-date small {\r\nfont-size: 12px;\r\n}\r\n\r\n.title-price {\r\ndisplay: flex;\r\njustify-content: space-between;\r\nalign-items: center;\r\nmargin: 0;\r\n}\r\n\r\n.price {\r\n  font-size: 14px;\r\n  color: #BC9CFF;\r\n}\r\n\r\n.sum {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  flex-wrap: wrap;\r\n  margin-top: 20px;\r\n\r\n  h2 {\r\n    font-size: 19px;\r\n  }\r\n}\r\n\r\n.info {\r\n  max-width: 230px;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.info:nth-child(3)::after, .info:nth-child(5)::after {\r\n  content: \"i\";\r\n  position: absolute;\r\n  text-align: center;\r\n  font-size: 12px;\r\n  line-height: 18px;\r\n  margin: 0 auto;\r\n  width: 20px;\r\n  height: 20px;\r\n  border: 1px solid rgba(31, 32, 65, 0.25);\r\n  border-radius: 50%;\r\n  box-sizing: border-box;\r\n  color: rgba(31, 32, 65, 0.25);\r\n}\r\n\r\n.money {\r\nmax-width: 80px;\r\n}\r\n","_mixins.scss":"@mixin body {\r\n  font-family: Montserrat, sans-serif;\r\n  font-weight: normal;\r\n  font-size: 14px;\r\n  line-height: 24px;\r\n  color: rgba(31, 32, 65, 0.75);\r\n  @font-face {\r\n  font-family: 'Material Icons';\r\n  font-style: normal;\r\n  font-weight: 400;\r\n  src: url(https://example.com/MaterialIcons-Regular.eot); /* For IE6-8 */\r\n  src: local('Material Icons'),\r\n    local('MaterialIcons-Regular'),\r\n    url(https://example.com/MaterialIcons-Regular.woff2) format('woff2'),\r\n    url(https://example.com/MaterialIcons-Regular.woff) format('woff'),\r\n    url(https://example.com/MaterialIcons-Regular.ttf) format('truetype');\r\n}\r\n\r\n}\r\n\r\n@mixin h1 {\r\n  font-size: 24px;\r\n  font-weight: bold;\r\n  line-height: 29px;\r\n  color: $DarkShade;\r\n}\r\n\r\n@mixin h2 {\r\n  font-weight: bold;\r\n  font-size: 19px;\r\n  line-height: 23px;\r\n  color: $DarkShade;\r\n}\r\n\r\n@mixin h3 {\r\n  font-weight: bold;\r\n  font-size: 12px;\r\n  line-height: 15px;\r\n  text-transform: uppercase;\r\n  color: $DarkShade;\r\n}\r\n","_colors.scss":"$DarkShade: #1F2041;\r\n$Purple: #BC9CFF;\r\n$Green: #6FCF97;"},"lineCount":null}},"error":null,"hash":"632fbcaee78d2125446dc4e6e2c181a4","cacheData":{"env":{}}}